<reactiveUi:ReactiveUserControl
    x:Class="Growl.Ex.View.GrowlLowerView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:hc="https://handyorg.github.io/handycontrol"
    xmlns:infrastructure="clr-namespace:Growl.Ex.ViewModel.Infrastructure"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:viewModel="clr-namespace:Growl.Ex.ViewModel"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:TypeArguments="viewModel:GrowlLowerViewModel"
    mc:Ignorable="d">
    <Grid
        Width="400"
        Height="220"
        Margin="10,10,10,10"
        HorizontalAlignment="Right"
        VerticalAlignment="Bottom"
        hc:Growl.Token="{x:Static infrastructure:MessageToken.GrowlLowerPanel}" />
</reactiveUi:ReactiveUserControl>
